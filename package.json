{
  "name": "srefs",
  "type": "module",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "build": "npm run sync:from-s3 && astro build && npm run sync:to-s3",
    "build:local": "astro build",
    "build:cdn": "PUBLIC_USE_CDN=true npm run build",
    "preview": "astro preview",
    "astro": "astro",
    "sync:from-s3": "tsx scripts/sync-from-s3.ts",
    "sync:to-s3": "tsx scripts/sync-to-s3.ts",
    "migrate:to-s3": "tsx scripts/migrate-to-s3.ts",
    "migrate:dry-run": "tsx scripts/migrate-to-s3.ts --dry-run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "bot:dev": "tsx watch src/bot/index.ts",
    "bot:start": "tsx src/bot/index.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@astrojs/react": "^4.3.0",
    "@astrojs/sitemap": "^3.5.0",
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/lib-storage": "^3.864.0",
    "@tailwindcss/vite": "^4.1.12",
    "@types/js-yaml": "^4.0.9",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "astro": "^5.13.2",
    "discord.js": "^14.21.0",
    "dotenv": "^17.2.1",
    "fuse.js": "^7.1.0",
    "js-yaml": "^4.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sharp": "^0.34.3",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^18.0.1",
    "vitest": "^3.2.4"
  }
}
